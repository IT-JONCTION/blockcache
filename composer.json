{
    "name": "itjonction/blockcache",
    "description": "Block caching for both laravel and vanilla php",
    "require-dev": {
      "phpunit/phpunit": "^11.0",
        "illuminate/database": ">=10.0 <13.0",
        "illuminate/cache": ">=10.0 <13.0",
        "illuminate/config": "^10.0 || ^13.0",
        "illuminate/container": "^10.0 || ^13.0",
        "mockery/mockery": "2.0.x-dev",
        "monolog/monolog": "^2.0"
    },
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Itjonction\\Blockcache\\": "src/"
        }
